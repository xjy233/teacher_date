<!--pages/me/me.wxml-->
<view class="page">
  <!--页头-->
  <view class="page__hd">
    <view class="head">
      <view class="userinfo-avatar">
        <open-data type="userAvatarUrl"></open-data>
      </view>
      <open-data type="userNickName"></open-data>
    </view>
  </view>
  <!--主体-->
   <view class="page__bd">
    <view class="list">
      <view class='list-opinion'>
        <navigator url="../history/history" class="navigator">
          <view class="navigator-text">历史记录</view>
          <view class="navigator-arrow"></view>
        </navigator>
      </view>
      <view class='list-opinion'>
        <navigator url="../opinion/opinion" class="navigator">
          <view class="navigator-text">谁联系了我</view>
          <view class="navigator-arrow"></view>
        </navigator>
      </view>
    </view>
  </view>
  <view class="page__bd">
    <view class="list">
      <view class='list-opinion'>
        <navigator url="../opinion/opinion" class="navigator">
          <view class="navigator-text">意见反馈</view>
          <view class="navigator-arrow"></view>
        </navigator>
      </view>
      <view class='list-opinion'>
        <navigator url="../about/about" class="navigator">
          <view class="navigator-text">关于蜜蜂</view>
          <view class="navigator-arrow"></view>
        </navigator>
      </view>
    </view>
  </view>
  <view class="page__bd">
    <view class="list">
      <view class='list-contact'>
        <navigator url="../opinion/opinion" class="navigator">
          <view class="navigator-text">联系客服</view>
          <view class="navigator-arrow-kefu"><image src='../../assets/images/lianxikefu.png'></image></view>
        </navigator>
      </view>
    </view>
  </view>

<!--没有页脚-->

</view>
